<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="SIE2.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/sample.gif"
               class="sie.actions.SampleAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="sie2.actions.SampleAction">
         </action>
         <action
               label="&amp;History Metrics Action"
               icon="icons/sample.gif"
               class="sie.actions.HistoryMetricsAction"
               tooltip="Calculate history metrics"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="sie2.actions.HistoryMetricsAction">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="SIE.category1"
            name="JavaExtractor">
      </category>
      <command
            id="SIE.command1"
            name="JavaExtractorCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="sie.snippets.MetricExtractorSnippet"
            commandId="SIE.handler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            contextId="org.eclipse.ui.contexts.window"
            schemeId="SIE.key1"
            sequence="SIE.key1">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
      </menuContribution>
   </extension>

</plugin>
